{
  "name": "unified-search-graphql-api",
  "version": "2.0.0",
  "description": "GraphQL API for unified search",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --ext ts src/index.ts --exec ts-node",
    "transpile": "tsc",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "serve": "node --max-http-header-size=16000 dist/index.js",
    "test": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@apollo/subgraph": "^2.1.4",
    "@graphql-tools/schema": "^9.0.9",
    "@sentry/node": "^6.16.1",
    "@types/luxon": "^2.0.9",
    "@types/node": "^18.11.9",
    "@types/prettier": "^3.0.0",
    "apollo-datasource-rest": "^3.7.0",
    "apollo-server": "^3.11.1",
    "apollo-server-express": "^3.11.1",
    "apollo-server-plugin-response-cache": "^3.8.1",
    "cors": "^2.8.5",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "express": "^4.17.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "luxon": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-jest": "^26.6.3",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^3.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^8.3.0",
    "typescript": "^5.2.2"
  }
}
