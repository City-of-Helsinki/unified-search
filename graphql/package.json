{
  "name": "unified-search-graphql-api",
  "version": "2.2.0",
  "description": "GraphQL API for unified search",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --ext ts src/index.ts --exec ts-node",
    "transpile": "tsc",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "serve": "node --max-http-header-size=16000 dist/index.js",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest/presets/js-with-babel",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "@apollo/subgraph": "^2.11.2",
    "@graphql-tools/schema": "^9.0.19",
    "@sentry/node": "^6.19.7",
    "@types/luxon": "^2.4.0",
    "@types/node": "^18.19.120",
    "@types/prettier": "^3.0.0",
    "apollo-datasource-rest": "^3.7.0",
    "apollo-server": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "apollo-server-plugin-response-cache": "^3.8.2",
    "cors": "^2.8.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.3",
    "express": "^4.21.2",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "luxon": "^2.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^26.6.3",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^8.10.2",
    "typescript": "^5.8.3"
  }
}
