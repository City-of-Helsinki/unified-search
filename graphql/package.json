{
  "name": "unified-search-graphql-api",
  "version": "3.2.2",
  "description": "GraphQL API for unified search",
  "main": "src/index.ts",
  "type": "module",
  "engines": {
    "node": ">=22.16.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "nodemon --ext ts src/index.ts --exec tsx",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "serve": "node --import ./dist/src/sentry-init.mjs --trace-warnings --max-http-header-size=16000 dist/src/index.js",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write ."
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/cache-control-types": "^1.0.3",
    "@apollo/datasource-rest": "^6.4.1",
    "@apollo/server": "^5.1.0",
    "@apollo/server-plugin-response-cache": "^5.0.0",
    "@apollo/subgraph": "^2.12.1",
    "@as-integrations/express5": "^1.1.2",
    "@eslint/js": "^9.39.1",
    "@sentry/node": "^10.25.0",
    "@stylistic/eslint-plugin": "^5.6.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-promise": "^7.2.1",
    "express": "^5.1.0",
    "globals": "^16.5.0",
    "graphql": "^16.12.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^8.1.0",
    "luxon": "^3.7.2",
    "vite": "^7.2.2",
    "vitest": "^4.0.10"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@vitest/coverage-v8": "4.0.10",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
