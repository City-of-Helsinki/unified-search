{
  "name": "unified-search-graphql-api",
  "version": "2.2.0",
  "description": "GraphQL API for unified search",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "nodemon --ext ts src/index.ts --exec tsx",
    "transpile": "tsc",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "serve": "node --max-http-header-size=16000 dist/src/index.js",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write ."
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apollo/datasource-rest": "^6.4.1",
    "@apollo/server": "^5.0.0",
    "@apollo/server-plugin-response-cache": "^5.0.0",
    "@apollo/subgraph": "^2.11.2",
    "@as-integrations/express5": "^1.1.2",
    "@sentry/node": "^6.19.7",
    "@types/luxon": "^3.6.2",
    "@types/node": "^18.19.120",
    "cors": "^2.8.5",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.3",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "luxon": "^3.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "3.2.4",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.6.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
